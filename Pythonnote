class DaifukuShop:
    """
    å¤§ç¦åº—ã‚¯ãƒ©ã‚¹
    
    å¤§ç¦ã®åœ¨åº«ç®¡ç†ã¨è²©å£²ã‚’è¡Œã†ã‚¯ãƒ©ã‚¹ã§ã™ã€‚
    
    :ivar owner_name: åº—é•·ã®åå‰
    :vartype owner_name: str
    :ivar stock: åœ¨åº«æ•°
    :vartype stock: int
    :ivar sold: ç´¯è¨ˆè²©å£²æ•°
    :vartype sold: int
    
    .. code-block:: python
    
        >>> shop = DaifukuShop("ã†ã•ã†ã•", 20)
        >>> shop.sell(5)
        True
    """
    
    def __init__(self, owner_name, stock):
        """
        å¤§ç¦åº—ã‚’åˆæœŸåŒ–ã™ã‚‹
        
        :param owner_name: åº—é•·ã®åå‰
        :type owner_name: str
        :param stock: åˆæœŸåœ¨åº«æ•°
        :type stock: int
        :raises ValueError: stockãŒè² ã®æ•°ã®å ´åˆ
        """
        if stock < 0:
            raise ValueError("åœ¨åº«æ•°ã¯0ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        self.owner_name = owner_name
        self.stock = stock
        self.sold = 0
    
    def sell(self, quantity, price=150, discount=0):
        """
        å¤§ç¦ã‚’è²©å£²ã™ã‚‹
        
        æŒ‡å®šã•ã‚ŒãŸå€‹æ•°ã®å¤§ç¦ã‚’è²©å£²ã—ã€å£²ä¸Šã‚’è¨ˆç®—ã—ã¾ã™ã€‚
        
        :param quantity: è²©å£²å€‹æ•°
        :type quantity: int
        :param price: å˜ä¾¡ï¼ˆãƒ‡ãƒ•ã‚©ãƒ«ãƒˆ: 150å††ï¼‰
        :type price: int
        :param discount: å‰²å¼•ç‡ 0.0-1.0ï¼ˆãƒ‡ãƒ•ã‚©ãƒ«ãƒˆ: 0ï¼‰
        :type discount: float
        :return: å£²ä¸Šé‡‘é¡ï¼ˆå‰²å¼•å¾Œï¼‰ã€‚åœ¨åº«ä¸è¶³ã®å ´åˆã¯None
        :rtype: int or None
        :raises ValueError: quantityãŒ0ä»¥ä¸‹ã®å ´åˆ
        
        .. code-block:: python
        
            >>> shop = DaifukuShop("ã†ã•ã†ã•", 20)
            >>> shop.sell(5)
            750
            >>> shop.sell(3, discount=0.1)
            405
        
        .. note::
           åœ¨åº«ä¸è¶³ã®å ´åˆã¯Noneã‚’è¿”ã—ã€åœ¨åº«ã¯å¤‰æ›´ã•ã‚Œã¾ã›ã‚“ã€‚
        
        .. seealso::
           :func:`restock` - åœ¨åº«ã‚’è£œå……ã™ã‚‹
        """
        if quantity <= 0:
            raise ValueError("å€‹æ•°ã¯1ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        if quantity > self.stock:
            return None
        
        total = int(quantity * price * (1 - discount))
        self.stock -= quantity
        self.sold += quantity
        
        return total
    
    def restock(self, quantity):
        """
        åœ¨åº«ã‚’è£œå……ã™ã‚‹
        
        :param quantity: è£œå……å€‹æ•°
        :type quantity: int
        :raises ValueError: quantityãŒ0ä»¥ä¸‹ã®å ´åˆ
        """
        if quantity <= 0:
            raise ValueError("è£œå……å€‹æ•°ã¯1ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        self.stock += quantity

