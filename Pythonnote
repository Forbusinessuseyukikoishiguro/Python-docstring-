def add(a, b):
    result = a + b  # â† Step Intoã™ã‚‹ã¨ã“ã“ã«å…¥ã‚‹
    return result

def multiply(x, y):
    temp = x * y
    return temp

def calculate(num1, num2):
    sum_result = add(num1, num2)  # â† ã“ã“ã§F7ã‚’æŠ¼ã™
    product = multiply(num1, num2)
    return sum_result, product

# ãƒ¡ã‚¤ãƒ³å‡¦ç†
answer = calculate(5, 3)  # â† æœ€åˆã®ãƒ–ãƒ¬ãƒ¼ã‚¯ãƒã‚¤ãƒ³ãƒˆ
print(answer)

