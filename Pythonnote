from typing import Optional, Dict

class DaifukuShop:
    """
    å¤§ç¦åº—ã‚¯ãƒ©ã‚¹
    
    å¤§ç¦ã®åœ¨åº«ç®¡ç†ã¨è²©å£²ã‚’è¡Œã†ã‚¯ãƒ©ã‚¹ã§ã™ã€‚
    
    Attributes:
        owner_name (str): åº—é•·ã®åå‰
        stock (int): åœ¨åº«æ•°
        sold (int): ç´¯è¨ˆè²©å£²æ•°
        revenue (int): ç´¯è¨ˆå£²ä¸Š
    
    Example:
        >>> shop = DaifukuShop("ã†ã•ã†ã•", 20)
        >>> shop.sell(5)
        750
    """
    
    def __init__(self, owner_name: str, stock: int) -> None:
        """
        å¤§ç¦åº—ã‚’åˆæœŸåŒ–ã™ã‚‹
        
        Args:
            owner_name (str): åº—é•·ã®åå‰
            stock (int): åˆæœŸåœ¨åº«æ•°
        
        Raises:
            ValueError: stockãŒè² ã®æ•°ã®å ´åˆ
        """
        if stock < 0:
            raise ValueError("åœ¨åº«æ•°ã¯0ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        self.owner_name = owner_name
        self.stock = stock
        self.sold = 0
        self.revenue = 0
    
    def sell(
        self,
        quantity: int,
        price: int = 150,
        discount: float = 0
    ) -> Optional[int]:
        """
        å¤§ç¦ã‚’è²©å£²ã™ã‚‹
        
        æŒ‡å®šã•ã‚ŒãŸå€‹æ•°ã®å¤§ç¦ã‚’è²©å£²ã—ã€å£²ä¸Šã‚’è¨ˆç®—ã—ã¾ã™ã€‚
        
        Args:
            quantity (int): è²©å£²å€‹æ•°
            price (int, optional): å˜ä¾¡ã€‚ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã¯150å††
            discount (float, optional): å‰²å¼•ç‡ï¼ˆ0.0-1.0ï¼‰ã€‚ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã¯0
        
        Returns:
            int: å£²ä¸Šé‡‘é¡ï¼ˆå‰²å¼•å¾Œï¼‰
            None: åœ¨åº«ä¸è¶³ã®å ´åˆ
        
        Raises:
            ValueError: quantityãŒ0ä»¥ä¸‹ã®å ´åˆ
        
        Example:
            >>> shop = DaifukuShop("ã†ã•ã†ã•", 20)
            >>> shop.sell(5)
            750
            >>> shop.sell(3, discount=0.1)
            405
        
        Note:
            åœ¨åº«ä¸è¶³ã®å ´åˆã¯Noneã‚’è¿”ã—ã€åœ¨åº«ã¯å¤‰æ›´ã•ã‚Œã¾ã›ã‚“ã€‚
        """
        if quantity <= 0:
            raise ValueError("å€‹æ•°ã¯1ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        if quantity > self.stock:
            return None
        
        total = int(quantity * price * (1 - discount))
        self.stock -= quantity
        self.sold += quantity
        self.revenue += total
        
        return total
    
    def get_stats(self) -> Dict[str, any]:
        """
        çµ±è¨ˆæƒ…å ±ã‚’å–å¾—ã™ã‚‹
        
        Returns:
            Dict[str, any]: ä»¥ä¸‹ã®ã‚­ãƒ¼ã‚’æŒã¤è¾æ›¸
                - owner (str): åº—é•·å
                - stock (int): åœ¨åº«æ•°
                - sold (int): ç´¯è¨ˆè²©å£²æ•°
                - revenue (int): ç´¯è¨ˆå£²ä¸Š
        """
        return {
            'owner': self.owner_name,
            'stock': self.stock,
            'sold': self.sold,
            'revenue': self.revenue
        }

