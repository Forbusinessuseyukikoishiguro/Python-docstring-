def calculate_sum(numbers):
    total = 0  # â† ã“ã“ã«ãƒ–ãƒ¬ãƒ¼ã‚¯ãƒã‚¤ãƒ³ãƒˆ
    for num in numbers:
        total += num  # â† ã“ã“ã§Step Overã§é€²ã‚€
    return total

result = calculate_sum([1, 2, 3, 4, 5])
print(result)

