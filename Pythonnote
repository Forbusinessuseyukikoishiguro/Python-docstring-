print(special_set)

print("\n--- å¤§ç¦ã®æ¯”è¼ƒ ---")
print(f"ã„ã¡ã”å¤§ç¦ == ã‚ã‚“ã“å¤§ç¦: {ichigo == anko}")  # __eq__
print(f"ã‚ã‚“ã“å¤§ç¦ < ã„ã¡ã”å¤§ç¦: {anko < ichigo}")  # __lt__
print(f"ã„ã¡ã”å¤§ç¦ã®å¤§ãã•: {len(ichigo)}æ–‡å­—")  # __len__

print("\n--- ãŠåº—ã®ç®¡ç† ---")
shop = DaifukuShop("ãµã‚ãµã‚å¤§ç¦åº—")
shop.add_daifuku(ichigo)
shop.add_daifuku(anko)
shop.add_daifuku(matcha)

print(f"åœ¨åº«æ•°: {len(shop)}å€‹")  # __len__
print(f"æœ€åˆã®å¤§ç¦: {shop[0]}")  # __getitem__
print(f"ã€Œã‚ã‚“ã“ã€ã¯ã‚ã‚Šã¾ã™ã‹ï¼Ÿ {'ã‚ã‚“ã“' in shop}")  # __contains__

print("\n--- å…¨å•†å“ãƒªã‚¹ãƒˆ ---")
for daifuku in shop:  # __iter__
    print(f"  ğŸ¡ {daifuku}")

