class Daifuku:
    """ãµã‚ãµã‚å¤§ç¦ã‚¯ãƒ©ã‚¹"""
    
    def __init__(self, filling, price):
        """å¤§ç¦ã‚’ä½œã‚‹ï¼ˆåˆæœŸåŒ–ï¼‰"""
        self.filling = filling  # ä¸­èº«
        self.price = price      # å€¤æ®µ
        print(f"ğŸ¡ {filling}å¤§ç¦ãŒã§ãã¾ã—ãŸï¼")
    
    def __str__(self):
        """å¤§ç¦ã‚’æ–‡å­—ã§èª¬æ˜ã™ã‚‹"""
        return f"{self.filling}å¤§ç¦ï¼ˆ{self.price}å††ï¼‰"
    
    def __repr__(self):
        """é–‹ç™ºè€…å‘ã‘ã®è¡¨ç¤º"""
        return f"Daifuku('{self.filling}', {self.price})"
    
    def __add__(self, other):
        """å¤§ç¦ã‚’è¶³ã—ç®—ã™ã‚‹ï¼ˆã‚»ãƒƒãƒˆå•†å“ï¼‰"""
        new_filling = f"{self.filling}+{other.filling}"
        new_price = self.price + other.price - 50  # ã‚»ãƒƒãƒˆå‰²å¼•
        return Daifuku(new_filling, new_price)
    
    def __eq__(self, other):
        """å¤§ç¦ãŒåŒã˜ã‹æ¯”è¼ƒã™ã‚‹"""
        return self.filling == other.filling
    
    def __lt__(self, other):
        """å€¤æ®µã‚’æ¯”è¼ƒã™ã‚‹ï¼ˆ< æ¼”ç®—å­ï¼‰"""
        return self.price < other.price
    
    def __len__(self):
        """å¤§ç¦ã®å¤§ãã•ï¼ˆæ–‡å­—æ•°ï¼‰ã‚’è¿”ã™"""
        return len(self.filling)
    
    def __call__(self):
        """å¤§ç¦ã‚’é£Ÿã¹ã‚‹ï¼"""
        return f"ã‚‚ãã‚‚ã...{self.filling}å¤§ç¦ã€ç¾å‘³ã—ã„ï¼ğŸ˜‹"


# ã†ã•ã†ã•åº—é•·ã®åº—
class DaifukuShop:
    """ãµã‚ãµã‚å¤§ç¦åº—"""
    
    def __init__(self, name):
        self.name = name
        self.daifukus = []
        print(f"ğŸª {name}é–‹åº—ã—ã¾ã—ãŸï¼")
    
    def __len__(self):
        """åœ¨åº«æ•°ã‚’è¿”ã™"""
        return len(self.daifukus)
    
    def __getitem__(self, index):
        """å¤§ç¦ã‚’å–ã‚Šå‡ºã™ï¼ˆã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã‚¢ã‚¯ã‚»ã‚¹ï¼‰"""
        return self.daifukus[index]
    
    def __setitem__(self, index, daifuku):
        """å¤§ç¦ã‚’ä¸¦ã¹ã‚‹"""
        self.daifukus[index] = daifuku
    
    def __contains__(self, filling):
        """ãã®å¤§ç¦ãŒã‚ã‚‹ã‹ç¢ºèª"""
        return any(d.filling == filling for d in self.daifukus)
    
    def __iter__(self):
        """å¤§ç¦ã‚’é †ç•ªã«è¦‹ã‚‹"""
        return iter(self.daifukus)
    
    def add_daifuku(self, daifuku):
        """å¤§ç¦ã‚’è¿½åŠ """
        self.daifukus.append(daifuku)


