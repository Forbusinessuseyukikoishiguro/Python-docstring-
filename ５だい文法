            raise ValueError("å€‹æ•°ã¯1ä»¥ä¸Šã«ã—ã¦ãã ã•ã„")
        
        if quantity > self.stock:
            return None
        
        total = int(quantity * price * (1 - discount))
        self.stock -= quantity
        self.sold += quantity
        
        return total
    
    def restock(self, quantity):
        """
        åœ¨åº«ã‚’è£œå……ã™ã‚‹
        
        Args:
            quantity (int): è£œå……å€‹æ•°
        
